@model int

@{
    ViewBag.Title = "Editar Ordem de Seriço";
}

<div class="container">
    <h2>Editar Ordem de Serviço</h2>
    @Html.Partial("_Form", @Model.ToString())
</div>

@section Scripts {
    <link href="https://cdn.datatables.net/1.10.12/css/jquery.dataTables.min.css" rel="stylesheet" />
    @Styles.Render("~/bundles/css/chosen")
    @Styles.Render("~/bundles/css/chosenBootstrap")

    <script src="https://cdn.datatables.net/1.10.12/js/jquery.dataTables.min.js" type="text/javascript"></script>
    @Scripts.Render("~/bundles/js/CadastrarOrdemServico")
    @Scripts.Render("~/bundles/js/chosen")

    <script type="text/javascript">
        $(function () {
            CadastrarOrdemServico.Iniciar();
        });
    </script>
}