@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Icone Estética Dentária</title>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="shortcut icon" type="image/x-png" href="/Images/icon.png">

    @*<link rel="icon" href="/Images/icon.ico" />
    <link rel="apple-touch-icon-precomposed" href="/Images/icon.ico" />*@


    @Styles.Render("~/bundles/css/bootstrap")
    @Styles.Render("~/bundles/css/toastr")
    @Styles.Render("~/bundles/css")
</head>
<body>
    <div class="container">
        <div id="MessageBoxJS"></div>
        <div id="loader" class="loading display-none">Aguarde,por favor</div>
        <div class="row text-center">
            <img src="~/Images/logoIcone.jpg" class="img-responsive display-inline logoLogin" />
        </div>
        <div class="row">
            <div class="col-sm-4"></div>
            <div class="col-sm-4">
                <form>
                    <div class="form-group">
                        <label for="txtLogin">Login:</label>
                        <input type="text" class="form-control" id="txtLogin" placeholder="Digite seu login" maxlength="20">
                    </div>
                    <div class="form-group">
                        <label for="txtSenha">Senha:</label>
                        <input type="password" class="form-control" id="txtSenha" placeholder="Digite sua senha" maxlength="20">
                    </div>
                    <button id="btnEntrar" type="button" class="btn btn-success">Entrar</button>
                </form>
            </div>
            <div class="col-sm-4"></div>
        </div>
    </div>

    @Scripts.Render("~/bundles/js/jquery")
    @Scripts.Render("~/bundles/js/bootstrap")
    @Scripts.Render("~/bundles/js/jquerymaskedinput")
    @Scripts.Render("~/bundles/js/toastr")


    @Scripts.Render("~/bundles/js/geral")
    @Scripts.Render("~/bundles/js/Account")

    <script type="text/javascript">
        $(function () {
            Inicializador.Iniciar();
            Account.Iniciar();
        });
    </script>
</body>
</html>
